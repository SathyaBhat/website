<!doctype html>
<html {% if section %} data-current="{{ section }}"{% endif %} lang="en">

	{% include "components/head.njk" %}

	<body class="text-gray-700 work-sans leading-normal text-base tracking-normal">
		<!--<section class="bg-white container main-container shadow-xl border border-gray-300 w-full mx-auto mt-8 mb-12 px-8 md:px-16 pb-16 pt-4">-->
		<section class="bg-dark">
		<div class="bg-dark">
		<div class="container main-container w-full mx-auto px-8 pb-3 md:px-16">	
		<div class="flex flex-wrap pb-4 text-xl">
            <div class="w-full md:w-full lg:w-1/2 xl:w-1/2 flex justify-left pt-12 md:pb-6">
                <a aria-label="A link back to home" href="{{ '/' | url }}">
					<img src="/static/img/cdkday_logo_with_date.png" style="width: 420px;" alt="CDK Day - 30th April 2021" class=""/>
				</a>
            </div>
			<div class="w-full md:w-full lg:w-1/2 xl:w-1/2 pt-3 md:pt-0 lg:pt-10 text-2xl text-gray-500">
				<a aria-label="A link back to eventbrite tickets for CDK Day" class="dark-link block float-none md:float-left lg:float-right" style="width:325px; margin: 0 auto;" href="https://www.eventbrite.com/e/cdk-day-tickets-145072211891">
					<img src="/static/img/ticket.svg" style="width: 60px; margin-right: 15px;" class="inline" alt="CDK Day Tickets" />
					<span style="padding-top:13px;">Tickets available now</span>
				</a>
			</div>
			{% include "components/nav.njk" %}
        </div>
		</div>
		</div>
        <!--<hr class="my-8"/>-->

			<main>
				{{ layoutContent | safe }}
			</main>

			{% include "components/footer.njk" %}
		</section>

	</body>

</html>
